package org.openmrs.module.chits;

import java.util.ArrayList;
import java.util.List;

import org.openmrs.User;

/**
 * Form for managing a user's authorized barangays.
 * 
 * @author Bren
 */
public class UserAuthorizedBarangaysForm {
	/**
	 * The user being viewed.
	 */
	private User user;

	/**
	 * The barangay codes the user is authorized to.
	 */
	private List<String> userBarangayCodes = new ArrayList<String>();

	public User getUser() {
		return user;
	}

	public void setUser(User user) {
		this.user = user;
	}

	public List<String> getUserBarangayCodes() {
		return userBarangayCodes;
	}

	public void setUserBarangayCodes(List<String> userBarangayCodes) {
		this.userBarangayCodes = userBarangayCodes;
	}
}
